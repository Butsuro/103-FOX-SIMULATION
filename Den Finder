#-------------------------------------THE ACTUAL FUNCTION--------------------------------------------------------

def find_items(container3, num):
    """Searches the enclosure (container) for a specific item (num) and returns their coordinates (1-based)."""
    item_locations = []

    if num not in valid_ids:
        return []  # return empty list if num is not a known ID

    for i, row in enumerate(container3):  # loop through rows (meters in height)
        for j, value in enumerate(row):  # loop through columns (meters in width)
            if value == num:  # Check if the item is present
                item_locations.append((i + 1, j + 1))  # convert to 1-based indexing

    return item_locations
    

# Find item locations (example: finding food with ID 6)
num = 1 # Change this to search for different items
found_items = find_items(container3, num)

# Display results
print("Enclosure Map (Each 0 represents 1 meter):")
for row in container3:
    print(" ".join(map(str, row)))

print(f"\nLocations of {num} ({valid_ids.get(num, 'Unknown')}):")
print(found_items)

