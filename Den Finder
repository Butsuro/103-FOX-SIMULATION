def find_den(Den3, num):
    """Searches the enclosure (container) for a specific item (num) and returns their coordinates (1-based)."""
    den_locations = []

    if num not in valid_ids:
        return []  # return empty list if num is not a known ID

    for i, row in enumerate(Den3):  # loop through rows (meters in height)
        for j, value in enumerate(row):  # loop through columns (meters in width)
            if value == num:  # Check if the item is present
                den_locations.append((i + 1, j + 1))  # convert to 1-based indexing

    return den_locations
    

# Find item locations (example: finding food with ID 6)
num = 1 # Change this to search for different items
found_den = find_den(Den3, num)

# Display results
print("Enclosure Map (Each 0 represents 1 meter):")
for row in Den3:
    print(" ".join(map(str, row)))

print(f"\nLocations of {num} ({valid_ids.get(num, 'Unknown')}):")
print(found_den)

#-------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

def find_overlapping_areas(container3, Den3):
    """Finds overlapping areas between two grids where the items match and are non-zero."""
    overlapping_areas = []

    # Ensure both grids are the same size
    if len(container3) != len(Den3) or len(container3[0]) != len(Den3[0]):
        return "Error: Grids must be of the same size."

    for i in range(len(container3)):  # Loop through rows
        for j in range(len(container3[i])):  # Loop through columns
            if container3[i][j] == Den3[i][j] and container3[i][j] != 0:  # Check for match and non-zero
                overlapping_areas.append((i + 1, j + 1))  # Add coordinates (1-based indexing)

    return overlapping_areas

# Test the function with your grids
overlapping_areas = find_overlapping_areas(container3, Den3)

# Display the results
print("Overlapping areas (1-based coordinates):")
if isinstance(overlapping_areas, list) and overlapping_areas:
    for area in overlapping_areas:
        print(area)
else:
    print("No overlapping areas found.")
