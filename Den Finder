def find_items(enclosure, num):
    """Searches the enclosure (container) for a specific item (num) and returns their coordinates (1-based)."""
    item_locations = []
    # Iterate through each cell in the enclosure (2D list)
    for i in range(len(enclosure)):
        for j in range(len(enclosure[i])):
            if enclosure[i][j] == num:
                # Appending coordinates with 1-based indexing
                item_locations.append((i, j))
    return item_locations
    

