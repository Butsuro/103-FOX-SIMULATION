def find_den(Den3, num):
    """Searches the enclosure (container) for a specific item (num) and returns their coordinates (1-based)."""
    den_locations = []

    if num not in valid_ids:
        return []  # return empty list if num is not a known ID

    for i, row in enumerate(Den3):  # loop through rows (meters in height)
        for j, value in enumerate(row):  # loop through columns (meters in width)
            if value == num:  # Check if the item is present
                den_locations.append((i + 1, j + 1))  # convert to 1-based indexing

    return den_locations
    

# Find item locations (example: finding food with ID 6)
num = 1 # Change this to search for different items
found_den = find_den(Den3, num)

# Display results
print("Enclosure Map (Each 0 represents 1 meter):")
for row in Den3:
    print(" ".join(map(str, row)))

print(f"\nLocations of {num} ({valid_ids.get(num, 'Unknown')}):")
print(found_den)

#-------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
# finding overlaping areas 

def find_overlapping_areas(container3, Den3):
    """Finds overlapping areas between two grids where the items match and are non-zero."""
    overlapping_areas = []

    # Ensure both grids are the same size
    if len(container3) != len(Den3) or len(container3[0]) != len(Den3[0]):
        return "Error: Grids must be of the same size."

    for i in range(len(container3)):  # Loop through rows
        for j in range(len(container3[i])):  # Loop through columns
            if container3[i][j] == Den3[i][j] and container3[i][j] :  # Check for match and non-zero
                overlapping_areas.append((i + 1, j + 1))  # Add coordinates (1-based indexing)

    return overlapping_areas

# Test the function with your grids
overlapping_areas = find_overlapping_areas(container3, Den3)

# Display the results
print("Overlapping areas (1-based coordinates):")
if isinstance(overlapping_areas, list) and overlapping_areas:
    for area in overlapping_areas:
        print(area)
else:
    print("No overlapping areas found.")

#--------------------------------------------------------------------------------------------------------------------------------
#eliminating overlaping areas 

def eliminate_repeats(found_items, found_den):
    # Using list comprehension and converting arrays to tuples for comparison
    return [arr for arr in found_items if tuple(arr) not in (tuple(inner_arr) for inner_arr in found_den)]

result = eliminate_repeats(found_items, found_den)
print("The new set of corrdinates is:")
print(result)

#-------------------------------------------------------------------------------------------------------------------------------------
# eliminate 2-meter radius 

def distance(coord1, coord2):
    # Calculate the Euclidean distance between two coordinates
    dist = math.sqrt((coord2[0] - coord1[0]) ** 2 + (coord2[1] - coord1[1]) ** 2)
    return dist

def remove_close_coordinates(found_items, found_den, radius=2):
    # Combine the two lists
    combined = found_items + found_den
    unique_coordinates = []

    for coord in combined:
        # Check the distance from all previously added coordinates
        too_close = False
        for unique_coord in unique_coordinates:
            dist = distance(coord, unique_coord)
            if dist <= radius:  # Condition is now inclusive of coordinates that are exactly 2 units away
                too_close = True
                print(f"Removing {coord} because it's within {radius} units of {unique_coord}. Distance = {dist}")
                break

        # Add the coordinate if it is not too close
        if not too_close:
            unique_coordinates.append(coord)

    return unique_coordinates

result = remove_close_coordinates(found_items, found_den)
print("\nFinal coordinates:")
print(result)
